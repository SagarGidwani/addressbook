pipeline{
    agent any

    stages{
        stage('compile'){
            steps{
                script{
                    echo 'compile-hello world'
                }
            }
        }
        stage('UnitTest'){
            steps{
                script{
                    echo 'UnitTest-hello world'
                }
            }
        }
        stage('package'){
            steps{
                script{
                    echo 'Pacakage-hello world'
                }
            }
        }
    }
}